<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="440.0" prefWidth="634.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="model.loginController">
   <children>
      <Label layoutX="131.0" layoutY="73.0" text="Welcome to Spartan Health" textFill="#0628bf">
         <font>
            <Font name="Calibri" size="33.0" />
         </font>
      </Label>
      <Label layoutX="131.0" layoutY="167.0" text="username: " />
      <Label layoutX="132.0" layoutY="210.0" text="password: " />
      <TextField fx:id="userTextField" layoutX="229.0" layoutY="162.0" prefHeight="31.0" prefWidth="229.0" />
      <PasswordField fx:id="passwordText" layoutX="229.0" layoutY="205.0" prefHeight="31.0" prefWidth="229.0" />
      <Button fx:id="adminLoginBtn" layoutX="177.0" layoutY="291.0" mnemonicParsing="false" onAction="#changeToAdmin" text="Admin Login" />
      <Button fx:id="loginBtn" layoutX="401.0" layoutY="291.0" mnemonicParsing="false" onAction="#changeToPtLookup" text="Login" />
      <Button layoutX="280.0" layoutY="386.0" mnemonicParsing="false" text="Register" />
      <Label layoutX="276.0" layoutY="352.0" text="New user? ">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="loginResult" layoutX="241.0" layoutY="243.0" textFill="RED" />
   </children>
</AnchorPane>
